<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
      x="0" y="0" 
      viewBox="0 0 10000 10" 
      preserveAspectRatio="xMinYMax slice">
	
	<defs>
		<!-- Filtro snmCarvedLine -->
	    <filter id="snmCarvedLine">
	      <!-- InnerShadowBlack. Offseteo el SourceAlpha 5 y 5 -->
	      <feOffset in="SourceAlpha" dx="1" dy="1" result="figuraoffseteada"/>
	      <!-- Garantizo que sea negra -->
	      <feFlood flood-color="#000000" flood-opacity="1" result="elFlood" />
	      <feComposite in="elFlood" in2="figuraoffseteada" operator="in" result="lanegra" />
	      <!-- Obtengo la sombra Interna -->
	      <feComposite in="SourceAlpha" in2="lanegra" operator="out" result="elInnerShadow" />
	      <feGaussianBlur in="SourceAlpha" stdDeviation="6" result="elBlurblanco" />
	      <feFlood flood-color="#FFFFFF" flood-opacity="0.25" result="elFloodBlanco" />
	      <feComposite in="elFloodBlanco" in2="elBlurblanco" operator="in" result="elbrilloblanco" />
	      <feComposite in2="SourceGraphic" in="elbrilloblanco" operator="out" result="white_glow" />
	      <feMerge>
	        <feMergeNode in="elInnerShadow" />
	        <feMergeNode in="white_glow" />
	      </feMerge>
	    </filter>
		<!-- Gradiente Carved Effect -->
		<linearGradient id="carvedgradLine" x1="0.25" y1="0" x2="0.75" y2="1">
			<stop offset="0" stop-color="#000000" stop-opacity="0.35"/>
			<stop offset="1" stop-color="#000000" stop-opacity="0.1"/>
		</linearGradient>
	</defs>
	<path filter="url(#snmCarvedLine)" d="M0,0 L10000,0 L10000,10 L0,10 z" />
  	<path fill="url(#carvedgradLine)"  d="M0,0 L10000,0 L10000,10 L0,10 z" />
  	<!--path fill="#c60000" d="M9000,0 L9020,0 L9040,40 L9000,40 z" /-->
</svg>